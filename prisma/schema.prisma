generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int      @id @default(autoincrement())
  firstName      String?
  lastName       String?
  userName       String?
  email          String?  @unique
  hash           String? 

  measuredThrows  measuredThrows[]
  @@map("users")
}

model measuredThrows {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  disc        String?
  color       String?
  throwtype   String?
  distance    Int
  userId      Int
  user Users @relation(fields: [userId], references: [id])

  @@map("measuredThrows")
}
