generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String?
}

model measuredThrows {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  disc        String?
  color       String?
  throwtype   String?
  distance    Int
}
